                        </m:msup>
                        <m:mo>×</m:mo>
                        <m:mn>3</m:mn>
                      </m:mrow>
                    </m:msqrt>
                    <m:mo>+</m:mo>
                    <m:msqrt>
                      <m:mrow>
                        <m:msup>
