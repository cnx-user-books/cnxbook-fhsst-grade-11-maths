              </m:mrow>
            </m:math>
          </item>
          <item id="uid31">
            <m:math overflow="scroll">
              <m:mrow>
                <m:mn>2</m:mn>
                <m:msup>
                  <m:mi>x</m:mi>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mo>+</m:mo>
                <m:mn>12</m:mn>
                <m:mi>x</m:mi>
