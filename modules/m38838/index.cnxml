              </m:mrow>
            </m:math>
          </item>
          <item id="uid30">
            <m:math overflow="scroll">
              <m:mrow>
                <m:msup>
                  <m:mi>x</m:mi>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mo>+</m:mo>
