              </m:mrow>
            </m:math>
          </item>
          <item id="uid59">
            <m:math overflow="scroll">
              <m:mrow>
                <m:mi>b</m:mi>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>θ</m:mi>
