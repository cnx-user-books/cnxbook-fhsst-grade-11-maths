                <m:mo>=</m:mo>
                <m:mo form="prefix">tan</m:mo>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>θ</m:mi>
                  <m:mo>+</m:mo>
                  <m:msup>
                    <m:mn>180</m:mn>
