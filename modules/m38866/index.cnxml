                    <media id="id310854_media" alt="">
                      <image mime-type="image/png" src="../../media/MG11C17_015.png" id="id310854_onlineimage" width="72"><!-- NOTE: attribute width changes image size online (pixels). original width is 72. --></image>
                      <image mime-type="application/postscript" for="pdf" src="../../media/MG11C17_015.eps" id="id310854_printimage"/>
