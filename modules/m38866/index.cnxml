                <m:mi>c</m:mi>
                <m:mo>(</m:mo>
                <m:mi>θ</m:mi>
                <m:mo>)</m:mo>
                <m:mo>=</m:mo>
                <m:mo form="prefix">cos</m:mo>
                <m:mi>θ</m:mi>
              </m:mrow>
            </m:math>
          </item>
          <item id="uid61">
            <m:math overflow="scroll">
              <m:mrow>
                <m:mi>d</m:mi>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>θ</m:mi>
                  <m:mo>)</m:mo>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:mo form="prefix">cos</m:mo>
                <m:mrow>
                  <m:mo>(</m:mo>
