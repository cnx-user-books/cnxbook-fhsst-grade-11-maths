                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>θ</m:mi>
                  <m:mo>+</m:mo>
                  <m:msup>
                    <m:mn>60</m:mn>
                    <m:mo>∘</m:mo>
